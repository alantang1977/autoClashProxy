mode: rule
mixed-port: 7890
allow-lan: false
log-level: error
ipv6: false
tcp-concurrent: true
unified-delay: false
geodata-loader: standard
geo-auto-update: true
geo-update-interval: 12
geodata-mode: true
geox-url:
  geoip: "https://github.com/Loyalsoldier/v2ray-rules-dat/releases/latest/download/geoip.dat"
  geosite: "https://github.com/Loyalsoldier/v2ray-rules-dat/releases/latest/download/geosite.dat"
  mmdb: "https://testingcf.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@release/country.mmdb"
  asn: "https://github.com/xishang0128/geoip/releases/download/latest/GeoLite2-ASN.mmdb"
external-controller-cors:
  allow-private-network: true
  allow-origins:
  - '*'
myProxyDNS: &proxyDNS
  - https://1.1.1.1/dns-query#DNS
  - https://8.8.8.8/dns-query#DNS
  - https://9.9.9.9/dns-query#DNS
  - https://185.222.222.222/dns-query#DNS
  - https://208.67.222.222/dns-query#DNS
myDirectDNS: &directDNS
  - 114.114.114.114
  - 119.29.29.29
  - 211.136.17.107
  - 211.136.20.203
  - 211.137.58.20
  - 211.137.64.163
  - 223.5.5.5
dns:
  enable: true
  ipv6: false
  enhanced-mode: fake-ip
  cache-algorithm: arc
  prefer-h3: false
  use-hosts: false
  use-system-hosts: false
  respect-rules: true
  listen: 0.0.0.0:53
  fake-ip-filter-mode: whitelist
  fake-ip-filter:
    - "geosite:gfw,tld-!cn,geolocation-!cn"
    - 'rule-set:proxy,myProxy'
  default-nameserver: *directDNS
  proxy-server-nameserver: *directDNS
  direct-nameserver: *directDNS
  nameserver: *directDNS
  nameserver-policy:
    'rule-set:direct,myDirect': *directDNS
    'geosite:cn,connectivity-check,private': *directDNS
    'rule-set:proxy,myProxy': *proxyDNS
    'geosite:gfw,tld-!cn,geolocation-!cn': *proxyDNS
profile:
  store-selected: true
  store-fake-ip: true
tun:
  auto-detect-interface: true
  auto-route: true
  device: Mihomo
  dns-hijack:
  - any:53
  enable: true
  mtu: 1500
  stack: mixed
  strict-route: false
proxy-providers:
  我的机场:
    type: file
    path: ./myOwn.yaml
    interval: 86400
proxies:
proxy-groups:
rule-providers:
  direct:
    behavior: domain
    interval: 14400
    path: ./ruleset/direct.txt
    type: http
    url: https://cdn.jsdelivr.net/gh/Loyalsoldier/clash-rules@release/direct.txt
  apple:
    behavior: domain
    interval: 14400
    path: ./ruleset/apple.txt
    type: http
    url: https://raw.githubusercontent.com/Loyalsoldier/clash-rules/release/apple.txt
  icloud:
    type: http
    behavior: domain
    interval: 14400
    path: ./ruleset/icloud.txt
    url: "https://cdn.jsdelivr.net/gh/Loyalsoldier/clash-rules@release/icloud.txt"
  proxy:
    behavior: domain
    interval: 14400
    path: ./ruleset/proxy.txt
    type: http
    url: https://raw.githubusercontent.com/Loyalsoldier/clash-rules/release/proxy.txt
  myProxy:
    behavior: domain
    interval: 3600
    path: ./ruleset/myProxy.txt
    type: file
  myDirect:
    behavior: domain
    interval: 3600
    path: ./ruleset/myDirect.txt
    type: file
rules:
- RULE-SET,myProxy,全球互联
- RULE-SET,myDirect,直连规则
- RULE-SET,direct,直连规则
- GEOSITE,cn,直连规则
- GEOSITE,connectivity-check,直连规则
- RULE-SET,apple,直连规则
- RULE-SET,icloud,直连规则
- GEOSITE,private,直连规则
- GEOSITE,category-speedtest@cn,直连规则
- GEOIP,CN,直连规则,no-resolve
- GEOSITE,twitter,TWITTER
- GEOSITE,youtube,YOUTUBE
- GEOSITE,tiktok,TIKTOK
- GEOSITE,category-forums,FORUMS
- GEOSITE,github,GITHUB
- GEOSITE,telegram,TELEGRAM
- GEOSITE,category-ai-!cn,AI
- GEOSITE,category-games-!cn,GAME
- GEOSITE,gfw,GFW
- GEOSITE,geolocation-!cn,全球互联
- GEOSITE,tld-!cn,全球互联
- GEOIP,telegram,TELEGRAM,no-resolve
- GEOIP,twitter,TWITTER,no-resolve
- RULE-SET,proxy,全球互联
- MATCH,漏网之鱼
