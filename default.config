mode: rule
mixed-port: 7890
allow-lan: false
log-level: debug
ipv6: false
external-controller: 127.0.0.1:34885
secret: d53df256-8f1b-4f9b-b730-6a4e947104b6
unified-delay: true
geo-auto-update: true
geo-update-interval: 4
external-controller-cors:
  allow-private-network: true
  allow-origins:
  - '*'
dns:
  enable: true
  ipv6: false
  enhanced-mode: fake-ip
  cache-algorithm: arc
  prefer-h3: false
  use-hosts: true
  use-system-hosts: true
  respect-rules: false
  listen: 0.0.0.0:53
  fake-ip-filter-mode: blacklist
  fake-ip-filter:
    - '*.lan'
    - "xbox.*.microsoft.com"
    - "+.xboxlive.com"
    - localhost.ptlogin2.qq.com
    - "geosite:cn"
  default-nameserver:
    - 119.29.29.29
    - 223.5.5.5
  nameserver:
    - 119.29.29.29
    - 114.114.114.114
    - https://doh.pub/dns-query
    - https://dns.alidns.com/dns-query
  nameserver-policy:
    "geosite:cn,private":
    - https://doh.pub/dns-query
    - https://dns.alidns.com/dns-query
  fallback:
    - 1.1.1.1
    - 8.8.8.8
    - https://cloudflare-dns.com/dns-query
    - https://dns.google/dns-query
    - tls://1.0.0.1:853
    - tls://dns.google:853
  fallback-filter:
    geoip: true
    geoip-code: CN
    geosite:
      - gfw
    ipcidr:
      - 240.0.0.0/4
    domain:
      - +.google.com
      - +.facebook.com
      - +.youtube.com
      - +.twitter.com
      - +.x.com
      - +.tiktok.com
      - +.xn--ngstr-lra8j.com
      - +.google.cn
      - +.googleapis.cn
      - +.gvt1.com
profile:
  store-selected: true
tun:
  auto-detect-interface: true
  auto-route: true
  device: Mihomo
  dns-hijack:
  - any:53
  enable: true
  mtu: 1500
  stack: system
  strict-route: false
proxies:
proxy-groups:
rule-providers:
  apple:
    behavior: domain
    interval: 14400
    path: ./ruleset/apple.yaml
    type: http
    url: https://cdn.jsdelivr.net/gh/Loyalsoldier/clash-rules@release/apple.txt
